<h1>Listing gtm_strategies</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gtm_strategies.each do |gtm_strategy| %>
      <tr>
        <td><%= gtm_strategy.user %></td>
        <td><%= link_to 'Show', gtm_strategy %></td>
        <td><%= link_to 'Edit', edit_gtm_strategy_path(gtm_strategy) %></td>
        <td><%= link_to 'Destroy', gtm_strategy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gtm strategy', new_gtm_strategy_path %>
