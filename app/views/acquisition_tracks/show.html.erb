<%- model_class = AcquisitionTrack -%>
<div class="page-header">
  <h1><%=t '.title', :default => AcquisitionTrack %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:user) %></dt>
      <dd><pre class="prettyprint"><%= @acquisition_track.user %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customer) %></dt>
      <dd><pre class="prettyprint"><%= @acquisition_track.customer %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:user_activity) %></dt>
      <dd><pre class="prettyprint"><%= @acquisition_track.user_activity %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customer_activity) %></dt>
      <dd><pre class="prettyprint"><%= @acquisition_track.customer_activity %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:plan_title) %></dt>
      <dd><pre class="prettyprint"><%= @acquisition_track.plan_title %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:date_scheduled) %></dt>
      <dd><pre class="prettyprint"><%= @acquisition_track.date_scheduled %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              acquisition_tracks_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_acquisition_track_path(@acquisition_track), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              acquisition_track_path(@acquisition_track),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

