<%- model_class = ProductStrategy -%>
<div class="page-header">
  <h1><%=t '.title', :default => ProductStrategy %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:feature_list) %></dt>
      <dd><pre class="prettyprint"><%= @product_strategy.feature_list %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:product_principles) %></dt>
      <dd><pre class="prettyprint"><%= @product_strategy.product_principles %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              product_strategies_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_product_strategy_path(@product_strategy), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              product_strategy_path(@product_strategy),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

