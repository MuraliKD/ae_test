<h1>Listing product_strategies</h1>

<table>
  <thead>
    <tr>
      <th>Feature list</th>
      <th>Product principles</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_strategies.each do |product_strategy| %>
      <tr>
        <td><%= product_strategy.feature_list %></td>
        <td><%= product_strategy.product_principles %></td>
        <td><%= product_strategy.user %></td>
        <td><%= link_to 'Show', product_strategy %></td>
        <td><%= link_to 'Edit', edit_product_strategy_path(product_strategy) %></td>
        <td><%= link_to 'Destroy', product_strategy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product strategy', new_product_strategy_path %>
